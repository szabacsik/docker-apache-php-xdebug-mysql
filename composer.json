{
  "type": "project",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "test/"
    }
  },
  "require": {
    "php": "^8.1",
    "monolog/monolog": "*",
    "ext-pdo": "*"
  },
  "require-dev": {},
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "server:start": [
      "php -S 0.0.0.0:8000 -t public"
    ]
  }
}